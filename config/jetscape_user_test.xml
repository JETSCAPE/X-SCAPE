<?xml version="1.0"?>

<jetscape>

  <enableAutomaticTaskListDetermination> false </enableAutomaticTaskListDetermination>

  <setReuseHydro> false </setReuseHydro>
  <!-- <nReuseHydro> 10 </nReuseHydro> -->

  <vlevel> 0 </vlevel>

  <nEvents> 2 </nEvents>

  <Random>
    <seed>1</seed>
  </Random>
  
  <outputFilename>ISRTestParticles</outputFilename>
  <JetScapeWriterAscii> on </JetScapeWriterAscii>
  <JetScapeWriterFinalStatePartonsAscii> on </JetScapeWriterFinalStatePartonsAscii>
  <JetScapeWriterFinalStateHadronsAscii> on </JetScapeWriterFinalStateHadronsAscii>
  <nEvents_printout>1</nEvents_printout>

  <!-- Inital State Module  -->
  <IS>
    <!--Trento> </Trento-->
    <MCGlauber>
        <nEvents_MCGla> 1 </nEvents_MCGla>
    </MCGlauber>
  </IS>

  <!-- Hard Process -->
  <Hard>
    <PythiaGun>
      <pTHatMin>100</pTHatMin>
      <pTHatMax>110</pTHatMax>
      <eCM>5020</eCM>
      <useHybridHad>1</useHybridHad>
    </PythiaGun>
  </Hard>

  <!--Preequilibrium Dynamics Module -->
  <Preequilibrium>
    <NullPreDynamics> </NullPreDynamics>
  </Preequilibrium>

  <!-- Hydro  Module  -->
  <Hydro>
    <Brick bjorken_expansion_on="false" start_time="0.6"> </Brick>
  </Hydro>

  <!--Eloss Modules -->
  <Eloss>

    <maxT>10</maxT>
    <Matter>
      <useHybridHad>1</useHybridHad>
      <in_vac> 0 </in_vac>
      <Q0> 2.0 </Q0>
      <vir_factor> 0.25 </vir_factor>
    </Matter>
  </Eloss>

  <!-- Jet Hadronization Module -->
  <!-- <JetHadronization>
    <name>colorless</name>
    <weak_decays>off</weak_decays>
  </JetHadronization> -->

</jetscape>
